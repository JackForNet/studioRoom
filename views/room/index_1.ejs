            <% }else{ %>
                <span>您好：<label  id="unn"><%=user.nick_name%></label></span>
            <a href="/login" class="land"> <img src="/images/header-pic07.png"/>登陆</a><a href="" class="register">
               <img src="/images/header-pic08.png"/>注册</a>
            <% } %>
            <span class="shouc"><img src="/images/header-pic09.png"/>收藏链接</span></div>
    </div>
    <!--header end-->
    <div class="r-main">
        <!--r-lft star-->
        <div class="r-lft lft">
            <a href="" class="a"><img src="images/r-lft-pic02.png"/><br>保存到桌面</a>
            <a class="a" id="b"><img src="images/r-lft-pic03.png"/><br>软件下载</a>
            <a href=""><img src="images/r-lft-pic04.png"/><br>财经日历</a>
            <a id="d"><img src="images/r-lft-pic05.png"/><br>课程安排</a>
            <a href=""><img src="images/r-lft-pic06.png"/><br>在线开户</a>
            <a href="" class="g"><img src="images/r-lft-pic08.png"/><br>手机直播室</a>
        </div>
        <div class="tanchu-bg"></div>
        <div class="tanchu-a"><img src="images/tanchu-a.jpg"/></div>
        <div class="tanchu-divc">
            <div class="bs tanchu" id="bs">
                <ul>
                    <li><img src="images/rjxz-pic01.png"/><a href=""><img src="images/rjxz-pic05.png"/></a><p>金山行情分析及交易软件</p></li>
                    <li><img src="images/rjxz-pic02.png"/><a href=""><img src="images/rjxz-pic05.png"/></a><p>金山行情分析及交易软件</p></li>
                    <li><img src="images/rjxz-pic03.png"/><a href=""><img src="images/rjxz-pic05.png"/></a><p>金山行情分析及交易软件</p></li>
                    <li><img src="images/rjxz-pic04.png"/><a href=""><img src="images/rjxz-pic05.png"/></a><p>金山行情分析及交易软件</p></li>
                </ul>
            </div>
            <div class="ds tanchu" id="ds" style="display:none;">
                <%-sysset.lesson%>
            </div>
        </div>
        <script type="text/javascript">
            $(function(){
                $(".r-lft a").click(function(){
                    $('.tanchu-bg, .tanchu-a, .tanchu-divc').css("display","block");
                    var liId = $(this).attr("id");
                    $("."+ liId +"s").css("display","block");
                });
                $(".tanchu-a").click(function(){
                    $(".tanchu-a, .tanchu-bg").css("display","none");
                    $(".tanchu-divc, .tanchu-divc > *").css("display","none");
                });
            })
        </script>
        <script>
            var navTop = parseFloat($(".r-lft").css("top"));
            $(window).scroll(function(){
                var scrollTop = document.documentElement.scrollTop||document.body.scrollTop;
                if(scrollTop >= navTop){
                    if(!-[1,]&&!window.XMLHttpRequest) {
                        $(".r-lft").css("top",scrollTop+"px");
                    }else{
                        $(".r-lft").css({"position":"fixed","top":"0px"});
                    }
                }else{
                    if(!-[1,]&&!window.XMLHttpRequest) {
                        $(".r-lft").css("top",navTop+"px");
                    }else{
                        $(".r-lft").css({"position":"absolute","top":navTop+'px'});
                    }
                }

            });
        </script>
        <!--r-lft end-->
        <!--r-ctr star-->
        <div class="r-ctr lft">
            <!--r-ctr-top star-->
            <div class="r-ctr-top">
                <ul>
                    <li class="curre">原油</li><li>白银</li><li>铜</li>
                </ul>
                <div class="curre-div">
                    <div>
                        <a href="" class="a"><img src="/images/r-ctr-pic01.png"/>20%看涨</a>
                        <a href="" class="b"><img src="/images/r-ctr-pic02.png"/>40%看涨</a>
                        <a href="" class="c"><img src="/images/r-ctr-pic03.png"/>40%看涨</a>
                    </div>
                    <div style="display:none;">
                        <a href="" class="a"><img src="/images/r-ctr-pic01.png"/>20%看涨</a>
                        <a href="" class="b"><img src="/images/r-ctr-pic02.png"/>20%看涨</a>
                        <a href="" class="c"><img src="/images/r-ctr-pic03.png"/>20%看涨</a>
                    </div>
                    <div style="display:none;">
                        <a href="" class="a"><img src="/images/r-ctr-pic01.png"/>20%看涨</a>
                        <a href="" class="b"><img src="/images/r-ctr-pic02.png"/>20%看涨</a>
                        <a href="" class="c"><img src="/images/r-ctr-pic03.png"/>20%看涨</a>
                    </div>
                </div>
            </div>
            <script type="text/javascript" >
                /*行情走势*/
                $(function(){
                    var $div_li =$(".r-ctr-top ul li");
                    $div_li.mouseover(function(){
                        $(this).addClass("curre")
                                .siblings().removeClass("curre");
                        var index =  $div_li.index(this);
                        $(".curre-div > div")
                                .eq(index).show()
                                .siblings().hide();
                    })
                })
            </script>
            <!--r-ctr-top end-->
            <!--r-ctr dl star-->
            <dl>
                <dt><img src="/images/r-ctr-pic04.png"/>实时行情</dt>
                <dd class="a"><span class="lft">现货白银</span><span class="rgt">现价3601</span></dd>
                <dd><span class="lft">涨跌幅</span><span class="rgt">↓ -0.55%</span></dd>
                <dd class="b"><span class="lft">现货铜</span><span class="rgt">现价31692</span></dd>
                <dd><span class="lft">涨跌幅</span><span class="rgt">↓ -0.11%</span></dd>
            </dl>
            <!--r-ctr dl end-->
            <!--r-ctr-btn star-->
            <div class="r-ctr-btn">
                <h2><img src="/images/r-ctr-pic05.png"/>在线人数</h2>
                <div id="userlist" style="overflow:auto;height: 200px">
                <ul id="User_List" class="content_2">

                </ul>
                </div>
            </div>
            <!--r-ctr-btn end-->
        </div>
        <!--r-ctr end-->
        <!--r-rgt star-->
        <div class="r-rgt rgt">
            <div class="r-rgt-top"><strong>视频直播</strong><span>刷新视频</span><p>咨询热线：400-8665 -322</p></div>
            <div class="video">
                <embed id="videolive" src="http://yy.com/s/34782949/34782949/yyscene.swf" quality="high" width="100%" height="100%" align="middle" allowscriptaccess="always" allowfullscreen="true" wmode="transparent" type="application/x-shockwave-flash" autostart="true">
            </div>
        </div>
        <!--r-rgt end-->
        <!--r-middle star-->
        <div class="r-middle" >
            <h2 style="display: flex"><img src="/images/r-middle-pic01.png"/> <MARQUEE scrollamount="3" width="90%"><p id="gundongspan"><%= sysset.gundonggonggao %></p></MARQUEE></h2>
           <div id="Y_PubMes_Div" style="overflow: auto;height:400px;">
            <ul id="message">

            </ul>
           </div>
            <div class="middle-btn">
                <p class="middle-ptop"><a href=""><img src="/images/r-middle-pic02.png"/></a><a href=""><img src="/images/r-middle-pic02.png"/></a><a href=""><img src="/images/r-middle-pic02.png"/></a><a href=""><img src="/images/r-middle-pic03.png"/></a><a href=""><img src="/images/r-middle-pic04.png"/></a></p>
                <p class="middle-pbtn"><a><img src="/images/r-middle-pic05.png"/><span class="emotion">表情</span></a><a style="cursor: pointer" onclick="CHAT.submit('xianhua')"><img src="/images/r-middle-pic06.png"/>送花</a></p>
                    <input id="Y_iSend_Input" class="send-text" type="text" maxlength="100" value="">
                    <input class="out_text" type="button" onclick="CHAT.submit()" value="发送">
            </div>
        </div>
        <!--r-middle end-->
    </div>
</div>
<!--mainimg star-->
<div class="mainimg">
    <!--zjtd star-->
    <div class="zjtd">
        <h2><img src="/images/zjtd-pic01.jpg"/><a href="">更多专家</a></h2>
        <ul>
            <% var i=0; resll.forEach(function(name,index){if(name.cat.indexOf('分析师')==0&&(index-i)<6){i++;%>
            <%if((index-i)==0){%>
            <li class="first"><img width="286" src="<%=name.tupian%>"><p><strong><%name.meta.forEach(function(dat){if(dat.key=="fxslevel"){%>
                            <%=dat.value%>
                    <%}})%>
                    </strong><%=name.title%></p></li>
            <%}else{%>
            <li><img width="286" src="<%=name.tupian%>"><p><strong><%name.meta.forEach(function(dat){if(dat.key=="fxslevel"){%>
                        <%=dat.value%>
                        <%}})%>
                    </strong><%=name.title%></p></li>
            <%}}})%>
        </ul>
    </div>
    <!--zjtd end-->
    <!--sphg star-->
    <div class="sphg">
        <h2><img src="/images/sphg-pic01.jpg"/><a href="">更多视频</a></h2>
        <dl>
            <%var j=0; resll.forEach(function(name,index){if(name.cat.indexOf('往期视频回顾')==0&&(index-j)<7){j++;%>
            <%if((index-j)==0){%>
            <dt><a target="_blank" href="<%=name.splink%>"><img width="553" src="<%=name.tupian%>"/><span><%=name.title%></span></a></dt>
            <dd>
            <%}else{%>
            <a target="_blank" href="<%=name.splink%>"><img width="259" src="<%=name.tupian%>"/><span><%=name.title%></span></a>
            <%}}})%>
            </dd>
        </dl>
    </div>
    <!--sphg end-->
    <!--jsjj star-->
    <div class="jsjj">
        <h2><img src="/images/jsjj-pic01.jpg"/><a href="">更多讲解</a></h2>
        <div>
            <%var k=0; resll.forEach(function(name,index){if(name.cat.indexOf('技术讲解集锦')==0&&(index-k)<7){j++;%>
            <%if((index-k)==0){%>
            <a target="_blank" href="<%=name.splink%>" class="first"><img src="<%=name.tupian%>"/></a>
            <%}else{%>
            <a target="_blank" href="<%=name.splink%>"><img width="210" src="<%=name.tupian%>"/></a>
            <%}}})%>
    </div>
    <!--jsjj end-->
</div>
